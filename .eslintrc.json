{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "eslint-disable-next-line": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "no-alert": "off",
    "no-underscore-dangle": "off",
    "react/react-in-jsx-scope": "off"
  }
}
